<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pdd.phoenix.mapper.TestDataMapper">

    <select id="findByCity" resultType="com.pdd.phoenix.entity.TestData" parameterType="java.lang.String">
        SELECT * FROM US_POPULATION WHERE CITY = #{city}
    </select>

</mapper>
